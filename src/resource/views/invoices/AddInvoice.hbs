        </div>
        <div class="formAdd">
            <form action="/managerInvoices/actAddNew" method="post">
                <h3 class="titleForm">Add new invoice</h3>
                <div class="title_search">
                    <p>Customer</p>
                    <div class="search1">
                        <input type="text" placeholder="Seach by name" class="search_customer" name="" id="">
                    </div>
                </div>

                <div class="infor person scroll1">
                    <div class="select_ps">
                        <div>
                            Id person
                        </div>
                        <div>
                            Person name
                        </div>
                        <div>
                            Phone number
                        </div>
                    </div>
                    {{#if customers}}
                    {{#each customers}}
                    <label class="select_ps" for="customer{{this._id}}">
                        <div>
                            {{this._id}}
                        </div>
                        <div class="name_customer">
                            {{this.name}}
                        </div>
                        <div>
                            {{this.numberPhone}}
                        </div>
                        <div class="o_click click_ps"></div>
                    </label>
                    <input type="radio" name="customer" value="{{this._id}}" id="customer{{this._id}}">
                    {{/each}}
                    {{/if}}
                </div>

                <div class="title_search">
                    <p>Products</p>
                    <div class="search1">
                        <input type="text" placeholder="Seach by name" class="search_product" name="" id="">
                    </div>
                </div>
                <div class="infor products scroll1">
                    <div class="select_ps">
                        <div>
                            Id person
                        </div>
                        <div>
                            Image
                        </div>
                        <div>
                            Name
                        </div>
                        <div>
                            Price
                        </div>
                        <div>
                            Quantity
                        </div>
                    </div>
                    {{#if products}}
                    {{#each products}}
                    <div class="select_ps">
                        <div>
                            {{this._id}}
                        </div>
                        <div>
                            <img class="img_pr" src="{{this.img}}" alt="" srcset="">
                        </div>
                        <div class="name_pr">
                            {{this.name}}
                        </div>
                        <div>
                            {{this.new_price}}
                        </div>
                        <div>
                            {{this.quantity}}
                        </div>
                        <div class="o_click click_pr"></div>
                    </div>
                    {{/each}}
                    {{/if}}
                </div>
                <div class="customer_r">
                    <div class="ps_id"></div>
                    <div class="ps_name"></div>
                    <div class="ps_phone"></div>
                </div>
                <div class="products_r">
                </div>
                <button class="addInvoice" type="submit">Add</button>
            </form>
        </div>
    </div>
</div>